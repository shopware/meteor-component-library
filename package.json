    "storybook": "^7.1.1",
    "storybook-dark-mode": "^3.0.1",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0",
